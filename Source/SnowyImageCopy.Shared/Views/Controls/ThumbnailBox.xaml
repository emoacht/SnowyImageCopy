<UserControl x:Class="SnowyImageCopy.Views.Controls.ThumbnailBox"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 Width="160" Height="120">
	<Grid>
		<Rectangle x:Name="Box"
				   Width="160" Height="120"
				   StrokeThickness="6"
				   StrokeDashArray="1.4 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2.4 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2.8 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2.8 1.6 2 1.6 2 1.6 2 1.6 2 1.6 2"
				   StrokeStartLineCap="Square" StrokeEndLineCap="square"
				   Stroke="Black"/>
	</Grid>
</UserControl>
